<div class="m-5">
  <h1>New Review</h1>

  <form action="/restaurants/:id/reviews"></form>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>


  <a href="/restaurants/:id" class="btn btn-secondary mt-2">Back</a>
</div>
